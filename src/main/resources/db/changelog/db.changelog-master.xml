                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

 <changeSet id="1" author="liquibase">
   <createTable tableName="petlist">
           <column name="id" type="int">
              <constraints primaryKey="true" nullable="false"/>
           </column>
           <column name="petName" type="varchar(255)">
           </column>
           <column name="code" type="varchar(255)">
              <constraints nullable="false" unique="true" uniqueConstraintName="petlist_code"/>
           </column>
           <column name="petType" type="varchar(255)">
           </column>
           <column name="furColor" type="varchar(255)">
           </column>
           <column name="country" type="varchar(255)">
           </column>
   </createTable>
